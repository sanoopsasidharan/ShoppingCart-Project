            function addToCart(proId) {

                $.ajax({
                    url: '/addTocarts/' + proId,
                    method: 'get',
                    success: (response) => {
                        if (response.status) {
                            const Toast = Swal.mixin({
                                toast: true,
                                position: 'top-end',
                                showConfirmButton: false,
                                timer: 2000,
                                didOpen: (toast) => {
                                    toast.addEventListener('mouseenter', Swal.stopTimer)
                                    toast.addEventListener('mouseleave', Swal.resumeTimer)
                                }
                            })
                            Toast.fire({
                                icon: 'success',
                                title: 'Product Add In Cart'
                            })


                            var count = response.cartCount

                            $('#coundtId').html(count)
                            count = parseInt(count) + 1
                            $('#coundtId').html(count)

                            $('#coundtId-mob').html(count)
                            $('#coundtId-mob').html(count)
                        } else {
                            const Toast = Swal.mixin({
                                toast: true,
                                position: 'top-end',
                                showConfirmButton: false,
                                timer: 2000,
                                didOpen: (toast) => {
