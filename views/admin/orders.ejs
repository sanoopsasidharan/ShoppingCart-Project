        <!-- pace js -->
        <script src="assets/libs/pace-js/pace.min.js"></script>

        <script src="assets/js/app.js"></script>
        <script>
            function statuschange(orderId){
                 var upsateStatus =$('#cars'+orderId).val()
                $.ajax({
                    url:'/admin/changeStatus',
                    data:{
                        upsateStatus:upsateStatus,
                        orderId:orderId
                    },
                    method:'post',
                    success:(response)=>{
                        if(response.status){
                            // $("#status"+orderId).load(location.href + " #status"+orderId);
                            $("#table"  ).load(location.href + " #table");
                        }
                    }
                })
                
            }
        </script>

