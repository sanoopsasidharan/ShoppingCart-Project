    <script>
        function dltCat(catName) {
            var tr = document.getElementById("tableRowOf" + catName);
            console.log(tr)
            var temData = { category: catName }
            Swal.fire({
                title: 'Are you sure?',
                text: "You won't be able to delete this!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, delete it!'
            }).then((result) => {
                if (result.isConfirmed) {
                    Swal.fire(
                        'Deleted!',
                        'Your category has been deleted.',
                        'success'
                    ),
                        $.ajax({
                            url: "/admin/deletecategory/",
                            method: "post",
                            data: temData,
                            success: (result) => {
                                if (result) {
                                    tr.style.display = "none";
                                } else {

                                }
                            }



                        })

                }
            })


        }
        $('#addCategory').submit((e) => {
            e.preventDefault()
            $.ajax({
                url: '/admin/addcategory',
                method: 'post',
                data: $('#addCategory').serialize(),
                success: (response) => {
                    if (response.status) {
                        location.reload()
                        // must be want reload
                        // $("#categroyTable").load(location.href + " #categroyTable");
                    } else {
                        Swal.fire(
                            'Already exists',
                            'Change the category name',
                            'error'
                        )

                    }
                }
            })
        })

        $('#exampleModal').on('show.bs.modal', function (event) {
            var myVal = $(event.relatedTarget).attr('data-val');
            console.log(myVal)
            $(this).find("#test").val(myVal);
        });

        // $('#staticBackdrop').on('show.bs.modal', function (event) {
        //     var myVal = $(event.relatedTarget).attr('data-val');
        //     console.log(myVal)
        //     $(this).find("#categoryname").val(myVal);
        // });


        $('#addcateOffer').submit((num)=>{
            num.preventDefault()
            $.ajax({
                url:'/admin/categoryAddCategoryOffer',
                data:$('#addcateOffer').serialize(),
                method:'post',
                success:(response)=>{
                    alert(response)
                }
                
            })
        })
