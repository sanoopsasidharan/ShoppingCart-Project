        <script>
            function blockUser(userId){
                var  check = confirm("are you sure to block");
                if(check){
                    $.ajax({
                   url:'/admin/blockuser',
                   data:{userId:userId},
                   method:'post',
                   success:(response)=>{
                       if(response.status){
                        location.reload()
                       }
                   }
               })
                } 
            }
            function unBlockUser(userId){
                $.ajax({
                    url:'/admin/unblockuser',
                    data:{userId:userId},
                    method:'post',
                    success:(response)=>{
                        if(response.status){
                            location.reload()
                        }
                    }
                })
            }
        </script>

